<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Update Venues</title>
  </head>
  <body>


    <h1>Venues:</h1>
    <hr />
    <h2>Edit Venue: @Model.GetName()</h2>
      <form action="/venues/edit/@Model.GetId()" method="post">
        <input type="hidden" name="_method" value="PATCH">
        <input name="venue-id" type="hidden" value="@Model.GetId()">
        <label for="venue-name">Category name</label>
        <input id="venue-name" name="venue-name" type="text">
        <button type="submit">Submit</button>
      </form>
    <h4><a href="/">Return home</a></h4>

  </body>
</html>
